{
  "mcpServers": {
    "AISearch": {
      "url": "http://appbuilder.baidu.com/v2/ai_search/mcp/sse?api_key=yourkey"
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ]
    },
    "jina-mcp-tools": {
      "command": "npx",
      "args": ["jina-mcp-tools"],
      "env": {
        "JINA_API_KEY": "your_jina_api_key"
      }
    },
    "tuzi-mcp": {
      "command": "uvx",
      "args": ["tuzi-mcp"],
      "env": {
        "TUZI_API_KEY": "your_tuzi_api_key"
      }
    }
  }
}